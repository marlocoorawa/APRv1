<UserControl x:Class="APR___Aplikasi_Perlombaan_Renang.Views.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:APR___Aplikasi_Perlombaan_Renang.Views"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Width="200" Margin="0,30,0,0" Style="{DynamicResource SquareButtonStyle}" Command="{Binding KelolaPerlombaanButton}">Kelola Perlombaan</Button>
            <Button Width="200" Margin="0,30,0,0" Style="{DynamicResource SquareButtonStyle}" Content="{Binding PageName}"></Button>
        </StackPanel>
    </Grid>
</UserControl>
